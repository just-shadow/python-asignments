class Person:
    def __init__(self, name, age, gender):
        self.name = name
        self.age = age
        self.gender = gender
        
    def introduce(self):
        
        print(f"Hello my name is {self.name} and I'm {self.age} years old and my gender is {self.gender}.")
              
person = Person("Sam", 20, "Male")
person.introduce()
